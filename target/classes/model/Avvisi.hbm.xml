<?xml version="1.0" encoding="UTF-8"?>

<hibernate-mapping>
	<class name="model.Avvisi" table="avvisi">
		<id name="id" type="int" column="id">
			<generator class="native"/>
		</id>
		<property name="avviso" type="string" column="avviso"/>
		<property name="data" type="string" column="data"/>
		<property name="nome_utente" type="string" column="nome_utente"/>
		<property name="destinatari" type="string" column="destinatari"/>
	</class>
</hibernate-mapping>